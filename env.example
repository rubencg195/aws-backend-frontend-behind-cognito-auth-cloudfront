# AWS Configuration
# Copy this file to .env and fill in your actual values

# AWS Region
REACT_APP_AWS_REGION=us-east-1

# Cognito Configuration
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_COGNITO_USER_POOL_WEB_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Lambda API Configuration
REACT_APP_LAMBDA_API_ENDPOINT=https://xxxxxxxxxx.lambda-url.us-east-1.on.aws

# Optional: Environment indicator
REACT_APP_ENVIRONMENT=development
